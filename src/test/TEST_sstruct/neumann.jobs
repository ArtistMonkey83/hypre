#=============================================================================
# sstruct: Run base 3d cases
#=============================================================================

mpirun -np 1 ./sstruct -in sstruct.in.neumann -r 6 6 6 -solver 10 \
 > neumann.out.0
mpirun -np 1 ./sstruct -in sstruct.in.neumann -r 6 6 6 -solver 11 \
 > neumann.out.1

#=============================================================================
# sstruct: Run various blockings and distributions
#=============================================================================

mpirun -np 4 ./sstruct -in sstruct.in.neumann -P 2 2 1 -b 1 3 2 -r 3 1 3 -solver 10 \
 > neumann.out.2
mpirun -np 3 ./sstruct -in sstruct.in.neumann -P 1 1 3 -b 6 1 1 -r 1 6 2 -solver 11 \
 > neumann.out.3
