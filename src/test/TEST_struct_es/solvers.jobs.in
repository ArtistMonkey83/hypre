DRIVER="./struct_es"

#echo Running $MPIRUN $DRIVER with $RUNPARM

#=============================================================================
# ij driver for eigenvaluesolvers: (wihtout -lobpcg option runs ij) 
# Run default case with all available PCG preconditioners (solvers): 
#    10: SMG (default)
#    11: PFMG
#    12: SparseMSG  
#    17: 2-step Jacobi
#    18: Diagonal scaling
#    19: none
#=============================================================================
#

#echo Running with -solver 10 -pcgitr 0
#echo PCG run...
$MPIRUN $DRIVER  -solver 10 -tol 1e-6 $RUNPARM > solvers.out.10
#echo LOBPCG run for one eigenpair...
$MPIRUN $DRIVER  -lobpcg -solver 10 -tol 1e-6 -pcgitr 0 -seed 1 -vrand 1 $RUNPARM > solvers.out.10.lobpcg 
#echo LOBPCG run for 5 eigenpairs...
$MPIRUN $DRIVER  -lobpcg -solver 10 -tol 1e-6 -pcgitr 0  -seed 1 -vrand 5 $RUNPARM  > solvers.out.10.lobpcg.5 

#echo Running with -solver 11 -pcgitr 0
